function tips_pop1(){
	var hheight = document.body.clientHeight;//网页可见区域高
	jQuery("#hidediv").css("height",hheight);
	jQuery("#hidediv").show();
	jQuery("#tcts1").show();
	jQuery("#tcts2").hide();
	//jQuery("body").css("overflow","hidden");
	jQuery("body").toggleClass("ovebody");
}
function tips_pop2() {
	var hheight = document.body.clientHeight;//网页可见区域高
	jQuery("#hidediv").css("height",hheight);
	jQuery("#hidediv").show();
	jQuery("#tcts2").show();
	jQuery("#tcts1").hide();
	//jQuery("body").css("overflow","hidden");
	jQuery("body").toggleClass("ovebody");
}
function toueme(id){
	jQuery("#"+id).hide();
	jQuery("#hidediv").hide();
	//jQuery("body").css("overflow","auto");
	jQuery("body").toggleClass("ovebody");
}

</script>
